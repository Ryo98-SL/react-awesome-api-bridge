{
  "name": "@ryo-98/react-api-bridge",
  "version": "1.0.0",
  "license": "MIT",
  "description": "A registry for component imperative api",
  "main": "dist/bridge.cjs",
  "author": "Deliang Shu deliangshu@yahoo.com",
  "scripts": {
    "build-publish": "npm run test && npm run build && npm publish --access public",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build:types": "tsc",
    "build": "tsx ./scripts/cmd.ts --env production",
    "build:dev": "tsx ./scripts/cmd.ts --env development --watch",
    "build:analyze": "tsx ./scripts/build.ts --env production --analyze",
    "serve": "tsx ./scripts/cmd.ts --env production --serve",
    "serve:dev": "tsx ./scripts/cmd.ts --env development --serve",
    "test": "npm run build && jest",
    "test-u": "npm run build && jest -u"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Ryo98-SL/react-awesome-api-bridge"
  },
  "exports": {
    ".": {
      "types": "./dist/bridge.d.ts",
      "import": "./dist/bridge.mjs",
      "require": "./dist/bridge.js"
    }
  },
  "peerDependencies": {
    "react": ">=16.0.0 <20.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.9",
    "@babel/core": "^7.23.9",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/preset-env": "^7.23.9",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/react": "^15.0.7",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.7",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@types/webpack": "^5.28.5",
    "@types/webpack-bundle-analyzer": "^4.7.0",
    "@types/webpack-dev-server": "^4.7.2",
    "@ungap/with-resolvers": "^0.1.0",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.1.3",
    "cross-env": "^7.0.3",
    "esbuild": "0.25.2",
    "fork-ts-checker-webpack-plugin": "^9.0.2",
    "html-webpack-plugin": "^5.6.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "react-dom": "^18.2.0",
    "react-router": "^6.23.1",
    "react-router-dom": "^6.23.1",
    "ts-jest": "^29.1.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.19.3",
    "typescript": "^5.4.5",
    "webpack": "^5.90.0",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-merge": "^5.10.0",
    "yargs": "^17.7.2"
  },
  "keywords": [
    "react",
    "api-bridge",
    "react-bridge",
    "react-api-bridge",
    "react-registry",
    "react-no-forwardRef",
    "react-action-manager",
    "typescript",
    "react-hooks",
    "中文文档"
  ]
}
