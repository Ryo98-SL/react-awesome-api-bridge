{
  "name": "@ryo-98/react-api-bridge",
  "version": "0.1.5",
  "license": "MIT",
  "description": "A registry for component imperative api",
  "main": "dist/bridge.js",
  "author": "Deliang Shu deliangshu@yahoo.com",
  "scripts": {
    "build-publish": "npm run build:webpack && npm publish --access public",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run build:types && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:webpack": "cross-env TS_NODE_PROJECT=\"tsconfig-for-webpack.json\" webpack --env mode=production && npm run build:types",
    "start": "cross-env TS_NODE_PROJECT=\"tsconfig-for-webpack.json\" webpack s --config webpack.config.ts --env mode=development"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Ryo98-SL/react-awesome-api-bridge"
  },
  "dependencies": {
    "react": ">=16.0.0 <20.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.9",
    "@babel/core": "^7.23.9",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/preset-env": "^7.23.9",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@types/node": "^20.12.7",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@types/webpack": "^5.28.5",
    "@types/webpack-dev-server": "^4.7.2",
    "babel-loader": "^9.1.3",
    "cross-env": "^7.0.3",
    "fork-ts-checker-webpack-plugin": "^9.0.2",
    "html-webpack-plugin": "^5.6.0",
    "react-dom": "^18.2.0",
    "react-router": "^6.23.1",
    "react-router-dom": "^6.23.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5",
    "webpack": "^5.90.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-merge": "^5.10.0"
  },
  "keywords": [
    "react",
    "react-bridge",
    "react-api-bridge",
    "react-registry",
    "react-no-forwardRef",
    "react-action-manager"
  ]
}
